{
  "name": "oink",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "release-android": "cd android && ./gradlew assembleRelease",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "update": "sudo npm install -g react-native-git-upgrade && react-native-git-upgrade"
  },
  "dependencies": {
    "firebase": "^4.8.0",
    "moment": "^2.18.1",
    "native-base": "^2.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native": "^0.50.4",
    "react-native-color-picker": "^0.4.0",
    "react-native-deprecated-custom-components": "git+https://github.com/facebookarchive/react-native-custom-components.git",
    "react-native-keyboard-aware-scroll-view": "^0.2.8",
    "react-navigation": "^1.0.0-beta.9"
  }
}
